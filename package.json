{
	"name": "@universalweb/utilitylib",
	"version": "1.0.0",
    "description": "Acidjs is a robust, modern, modular, & async focused Javascript utility library.",
    "keywords": [
        "Acid",
        "utility",
        "util",
        "modules",
        "stdlib"
    ],
    "homepage": "https://acidjs.com",
    "bugs": {
		"url": "https://github.com/universalweb/utilitylib/issues"
    },
    "repository": {
        "type": "git",
		"url": "git+https://github.com/universalweb/utilitylib.git",
        "directory": "package"
    },
    "license": "ISC",
    "maintainers": [
        {
            "name": "UniversalWeb",
            "email": "info@universalweb.io",
            "url": "https://universalweb.io"
        }
    ],
    "exports": {
        ".": {
            "node": {
                "require": "./index.js",
                "import": "./module/index.js"
            },
            "default": "./browser.js"
        }
    },
    "main": "./index.js",
    "module": "./module/index.js",
    "browser": "./browser.js",
    "bin": {
		"@universalweb/utilitylib": "./index.js"
    },
    "publishConfig": {
		"access": "public",
		"registry": "https://npm.pkg.github.com"
    },
    "scripts": {
        "start": "node index.js"
    },
    "company": "Universal Web",
    "readme": "README.md"
}
